#include <iostream>
using namespace std;

int main() {
    int a;
    std::cin >> a;
for (int row = 1; row <= a; row++) {
    for (int column = 1; column <= a; column++){
        if (column == 1 || column == a || row == 1 || row == a)
          {
            std::cout << a;
          }
          else 
          {
            std::cout << " ";
          }
    }
    std::cout << endl;
 }
}
