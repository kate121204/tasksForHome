#include <iostream>
using namespace std;

int main ()
{
    int number, reversedNumber=0, remaining;      
    std::cin>>number;    
 
   while(number != 0)    
   {    
     remaining = number % 10;      
     reversedNumber = reversedNumber * 10 + remaining;    
     number /= 10;    
   }    
  
  std::cout<<reversedNumber; 

}
