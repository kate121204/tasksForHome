#include <iostream>
using namespace std;

int main ()
{
  char symbol;
  int counter = 0;
  
  for (int i = 1; i <= 5;)
  {
    std::cin >> symbol;
    counter++;

    if (counter == 2)
    {
        std::cout << "No please don't do it!";
    }
    else if (counter == 3)
    {
        std::cout << "Don't do this to me!";
    }
    else if (counter == 4)
    {
        std::cout << "This is your last chance!";
    }
    else if (counter == 5)
    {
        for (int i = 1; i <= 10; i++)
        {
            std::cout << symbol;
        }
    }
  }
} 
