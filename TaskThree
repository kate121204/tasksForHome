#include <iostream>
using namespace std;

int main() {

char operation;
double a,b;

while (operation != '/' && b != 0){
   std::cin >> operation;
   std::cin >> a >> b;

   switch (operation){
     case '+':
      std::cout << "= ";
      std::cout << a + b;
     break;
     case '-':
      std::cout << "= ";
      std::cout << a - b;
     break;
     case '*':
      std::cout << "= ";
      std::cout << a * b;
     break;
     case '/':
      std::cout << "= ";
      std::cout << a / b;
     break;
   }
 }
}
