#include <iostream>
using namespace std;

int main() {

char operation;
double a,b;

std::cin >> operation;
std::cin >> a >> b;

switch (operation){
    case '+':
    std::cout << "= ";
    std::cout << a + b;
    break;
    case '-':
    std::cout << "= ";
    std::cout << a - b;
    break;
    case '*':
    std::cout << "= ";
    std::cout << a * b;
    break;
    default:
    if(b == 0){
        break;
    }
    std::cout << "= ";
    std::cout << a / b ;
    break;
 }
}

 
